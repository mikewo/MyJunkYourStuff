<configuration>
  <configSections>
  </configSections>
  <connectionStrings>
  </connectionStrings>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    
    <!-- Location Info -->
    <add key="Location" value="CodeMash"/>

    <!-- Image Base Uri Source -->
    <add key="Image.baseUri" value="https://{YourStorageAccountName}.blob.core.windows.net/locationimages/" />
    
    <!-- ImageRepository --> 
    <add key="Blob.storageAccountKey" value="{YourStorageKey}" /> 
    <add key="Blob.storageAccountName" value="{YourStorageAccountName}" /> 
    <add key="Blob.containerName" value="locationimages" />
        
    <!-- DocumentDB -->
    <add key="endpointUrl" value="https://{YOUR-DOCUMENTDB-ACCOUNT}.documents.azure.com:443/" />
    <add key="authKey" value="{YOUR-ACCOUNT-KEY}" />
    <add key="databaseId" value="MyJunkYourStuff" />
    <add key="collectionId" value="Junk" />
    
  </appSettings>
  <system.web>
